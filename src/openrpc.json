{
  "openrpc": "1.0.0",
  "info": {
    "title": "Iframe Execution Environment OpenRPC Transport",
    "version": "1.0.0"
  },
  "methods": [
    {
      "name": "connect",
      "description": "connect to transport",
      "summary": "connect transport",
      "params": [
        {
          "name": "uri",
          "description": "the uri to connect to",
          "required": true,
          "schema": {
            "type": "string",
            "format": "uri"
          }
        }
      ],
      "result": {
        "name": "successful",
        "schema": true
      }
    },
    {
      "name": "sendData",
      "description": "send data across the transport",
      "params": [
        {
          "name": "data",
          "description": "data to send",
          "schema": true
        }
      ],
      "result": {
        "name": "result",
        "schema": true
      }
    },
    {
      "name": "close",
      "description": "close the transport",
      "params": [],
      "result": {
        "name": "result",
        "schema": true
      }
    }
  ]
}
